Obstacle::Obstacle(int posX, int posY, int height, int width, string name){
	mCollider.x = posX;
	mCollider.y = posY;
	mCollider.h = height;
	mCollider.w = width;
	w = width;
	h = height;
	x = posX;
	y = posY;
	typeName = name;
	loadTexture();
}

bool loadTexture(string path){
	bool success = true;
	if( !gTexture.loadFromFile( path ) )
	{
		printf( "Failed to load texture image!\n" );
		success = false;
	}
}
